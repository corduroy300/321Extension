{
    "name": "Productivity Extension",
    "description": "Google Chrome extension that keeps track on user productivity",
    "version": "1.0.0",
    "manifest_version": 2,
    "icons": {
        "16": "/icons/sample_icon_16.png",
        "32": "/icons/sample_icon_32.png",
        "48": "/icons/sample_icon_48.png",
        "128": "/icons/sample_icon_128.png"
    },
    "background": {
        "scripts": [
            "/js/jquery-3.6.0.min.js",
            "/js/background.js",
            "/js/chart.min.js"
        ]
    },
    "options_page": "/html/options.html",
    "browser_action": {
        "default_popup": "/html/popup.html"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "notifications",
        "windows"
    ],
    "web_accessible_resources": [
        "fonts/*.woff2"
    ]
}